@model CI_Project.Entities.ViewModels.UserProfileModel
@{
	ViewData["Title"] = "404 Page Not Found";
	var userId = Context.Session.GetString("UserId");
	var fullName = Context.Session.GetString("FullName");

}

@section Styles {

	<link rel="Stylesheet" href="@Href("~/css/story.css")" />
}
<main class="position-relative">
	<header id="header" class="border-2 border-bottom ci-page-header bg-white w-100 z-3"
			style="background-color: transparent;min-width: max-content;">
		<partial name="_Navbar"></partial>
	</header>

	<div id="scrollable-area" class="overflow-y-scroll">


		<div class="container " style="max-width:var(--max-width);">
			<div class="d-flex flex-column justify-content-center align-items-center">
				<div class="d-flex flex-column justify-content-center align-items-center">
					<img src="~/images/404-Page-image.png" class="h-100 w-100" style="height:400px !important;width:400px !important;" />
					<p class="fs-4 ">PageNotFound</p>
				</div>
				<div class="d-md-block d-none">
					<p class="fs-1"><span class="fs-1 fw-bolder">Oops!</span>The page you were looking for doesn't exist.</p>
				</div>
				<div class="d-md-block d-none">
					<p class="fs-5">You may have misstyped the address or the page may have moved.</p>
				</div>
				<a class="border border-2 border-warning text-decoration-none text-warning rounded-pill py-2 px-3 mt-5 mt-md-0" asp-action="Index" asp-controller="Home">
					<span class="px-2">Go to Homepage</span>
					<img class="pe-2"  src="~/images/right-arrow.png" />
				</a>
			</div>
		</div>


	</div>


	<partial name="_Footer"></partial>
</main>

@section Scripts{
	<script src="~/js/headerSidebar.js"></script>
	<script src="~/js/scrolable.js"></script>
}